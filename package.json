{
  "name": "geonardo_dimappio",
  "version": "0.0.1",
  "description": "A tool to help simulate GPS with the Android Emulator.",
  "main": "index.js",
  "author": "Ryan Bosinger",
  "license": "MIT",
  "scripts": {
    "start": "nodemon server.js",
    "build:frontend": "bun build index.tsx --outdir=dist --minify && bun run bin/copy-html-for-dist.js",
    "watch:frontend": "nodemon --watch index.tsx --watch index.html --exec \"bun run build:frontend\""
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "dependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "leaflet": "^1.9.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "telnet-client": "^1.4.11",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "nodemon": "^2.0.22"
  }
}
